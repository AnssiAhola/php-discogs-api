{
    "name": "ricbra/php-discogs-api",
    "description": "The Discogs API makes it easy for developers to communicate with the Discogs platform",
    "type": "library",
    "keywords": ["discogs", "api"],
    "license": "MIT",
    "require": {
        "php": ">=5.4.0",
        "guzzlehttp/guzzle": "~4.0",
        "guzzlehttp/guzzle-services": "~0.3.0",
        "guzzlehttp/oauth-subscriber": "~0.1.2"
    },
    "require-dev": {
        "phpunit/phpunit": "~4"
    },
    "authors": [
        {
            "name": "Richard van den Brand",
            "email": "richard@vandenbrand.org"
        }
    ],
    "autoload": {
        "psr-0": { "Discogs": "lib/" }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
